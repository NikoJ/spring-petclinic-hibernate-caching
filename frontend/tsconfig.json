{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": false,
    "preserveConstEnums": true,
    "removeComments": true,
    "target": "ES6",
    "allowJs": true,
    "outDir": "public/dist",
    "sourceMap": true,
    "moduleResolution": "node"
  },
    "exclude": [
      "./node_modules/**/*"
    ]
}
